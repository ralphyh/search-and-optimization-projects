    "def bisec_opt(func, a, b):\n",
    "    minimum = 0.345\n",
    "    while b - a > minimum:\n",
    "        alpha = (a + b) / 2\n",
    "        y_alpha = func(alpha)\n",
    "        if y_alpha > 0:\n",
    "            b = alpha\n",
    "        elif y_alpha < 0:\n",
    "            a = alpha\n",
    "        else:\n",
    "            return alpha\n",
    "    # Return the mean of the two\n",
    "    return (a + b) / 2"
   ]
